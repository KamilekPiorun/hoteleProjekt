    <body class="login example1">

        <div class="main-login col-md-4 col-md-offset-4 col-sm-6 col-sm-offset-3">

            <!-- start: LOGIN BOX -->

            {% trans_default_domain 'FOSUserBundle' %}

            {% block fos_user_content %}
                {% if error %}
                    <div>{{ error.messageKey|trans(error.messageData, 'security') }}</div>
                {% endif %}
                <div class="box-login" style="display:block">
                    <h3>Zaloguj się</h3>

                    <fieldset>    
                        <form action="{{ path("fos_user_security_check") }}" method="post">

                            <div class="form-group">
                                <span class="input-icon">
                                    <input type="text" value="{{ last_username }}" class="form-control" id="username"required="required"  name="_username" placeholder="Nazwa użytkownika">
                                    <i class="fa fa-user"></i> </span>
                            </div>

                            <div class="form-group form-actions">
                                <span class="input-icon">
                                    <input type="password" class="form-control password" id="password" name="_password" required="required" placeholder="Hasło">
                                    <i class="fa fa-lock"></i>
                                    <a class="forgot" href="?box=forgot">
                                        Zapomniałem hasła
                                    </a> </span>
                            </div>
                            <input type="hidden" name="_csrf_token" value="{{ csrf_token }}" />

                            <div class="form-actions">
                                <label for="remember" class="checkbox-inline">
                                    <input type="checkbox" id="remember_me" name="_remember_me" value="on" />
                                    Zapamiętaj mnie
                                </label>
                                <button type="submit" id="_submit" name="_submit" value="{{ 'security.login.submit'|trans }}" class="btn btn-bricky pull-right">
                                    Login <i class="fa fa-arrow-circle-right"></i>
                                </button>
                            </div>
                        </form>
                    </fieldset>     
                {% endblock fos_user_content %}



            </div>
